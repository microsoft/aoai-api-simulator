POST {{aoai_endpoint}}/openai/deployments/{{aoai_deployment}}/chat/completions?api-version=2023-12-01-preview
Content-Type: application/json
api-key: {{aoai_key}}

{
  "messages": [{
    "role": "user",
    "content": "What is the meaning of life?"
    }, {
      "role": "assistant",
      "content": "blank"
    }, {
      "role": "user",
      "content": "What is the meaning of life?"
    }],
  "model": "gpt-5-turbo-1"
}

